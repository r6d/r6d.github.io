---
layout: none
---

{% for post in site.posts %}
{
    "type": "post",
        "title": "{{ post.title }}",
        "url": "{{ site.url }}{{ post.url }}",
        "path": "{{ post.url }}",
        "content": "{{ post.content | strip_html | strip_newlines}}",
        "categories": "{{ post.categories }}",
        "date": "{{ post.date }}"
}{% if forloop.last != true %},{% endif %}
{% endfor %}
